{
  "name": "dhmwheels",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "npm": "^5.10.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/styles/scss -o src/styles/css",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/styles/scss -o src/styles/css --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3"
  }
}
